@model ClickerHeroesTrackerWebsite.Models.Dashboard.UploadDataSummary

<h4>Recent Uploads:</h4>
<table>
    <tr>
        <th>Upload Time</th>
        <th></th>
    </tr>
    @foreach (var upload in Model.Uploads)
    {
        <tr>
            <td>@TimeZoneInfo.ConvertTime(upload.UploadTime, TimeZoneInfo.Utc, Model.UserSettings.TimeZone).ToString("G")</td>
            <td>@Html.ActionLink("View", "View", "Calculator", new { uploadId = upload.UploadId }, null)</td>
        </tr>
    }
</table>